{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "variable-name": false,
    "import-name": false,
    "align": [
      "arguments"
    ],
  }
}
