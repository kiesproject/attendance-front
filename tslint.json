{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "ordered-imports": true,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "variable-name": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
